<div id="<%= dom_id entity_info %>">
  <p>
    <strong>Assigned code:</strong>
    <%= entity_info.assigned_code %>
  </p>

  <p>
    <strong>Entity name:</strong>
    <%= entity_info.entity_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= entity_info.email %>
  </p>

  <p>
    <strong>Contact number:</strong>
    <%= entity_info.contact_number %>
  </p>

  <p>
    <strong>User:</strong>
    <%= entity_info.user_id %>
  </p>

  <p>
    <strong>Active status:</strong>
    <%= entity_info.active_status %>
  </p>

  <p>
    <strong>Del status:</strong>
    <%= entity_info.del_status %>
  </p>

</div>
